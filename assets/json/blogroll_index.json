---
layout: none
permalink: /blogroll/index.json
---
[
  {% assign friends = site.data.blogroll.friends %}
  {% for friend in friends %}
  {% unless friend.name contains 'Rizqi Nur Assyaufi' %}
    {
      "name":{{ friend.name | jsonify }},
      "github_username":{{ friend.github_username | jsonify }},
      "photo":{{ friend.photo | jsonify }},
      "detail":{{ friend.detail | jsonify }},
      "urls":{{ friend.urls | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endunless %}
  {% endfor %}
]
